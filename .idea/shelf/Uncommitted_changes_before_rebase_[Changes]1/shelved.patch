+    private void navigateTo(Stage stage, String fxmlFile) {
+        try {
+            FXMLLoader fxmlLoader = new FXMLLoader(getClass().getResource(fxmlFile));
+            Parent root = fxmlLoader.load();
+            Scene scene = new Scene(root, 1225, 745);
+            accueil controller = fxmlLoader.getController();
+            controller.setStage(stage);
+            stage.setScene(scene);
+            stage.setTitle("Accueil");
+            stage.show();
+        } catch (IOException e) {
+            showAlert("Erreur de chargement", "Impossible de charger le fichier FXML", e.getMessage());
+            e.printStackTrace();
+        }
+    }
+
+    private void showAlert(String title, String header, String content) {
+        Alert alert = new Alert(AlertType.ERROR);
+        alert.setTitle(title);
+        alert.setHeaderText(header);
+        alert.setContentText(content);
+        alert.showAndWait();
